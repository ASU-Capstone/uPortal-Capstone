    <portlet>
        <portlet-name>DynamicRespondrSkin</portlet-name>
        <display-name xml:lang="en">Dynamic Respondr Skin</display-name>
        <portlet-class>org.springframework.web.portlet.HeaderHandlingDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>classpath:/properties/contexts/portlet/DynamicRespondrSkin-portlet.xml</value>
        </init-param>
        <!-- TODO look into expiration-cache mapping.  An ActionRequest to update the preferences should invalidate
             the portal's render cache, but it does not appear to be doing that at least in CONFIG mode.
             Expiration cache is currently disabled so preference changes will take effect immediately. However
             this results in unnecessary processing of each render request.  UP-3986 -->
        <expiration-cache>0</expiration-cache>
        <cache-scope>private</cache-scope>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>config</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>Dynamic Respondr Skin</title>
        </portlet-info>
        <portlet-preferences>
            <!-- PREF is a prefix to dynamically select those portlet preference values that are sent to the
                 config jsp page, and written to the LESS file as variables without the prefix. -->

            <!-- Set to true to enable dynamic skins, which will process {PREFdynamicSkinName}.less with LESS variables
                 defined by portlet preferences to create a custom skin CSS file to embed into the page.
                 Set to empty string to have the page load {PREFdynamicSkinName}.css. -->
            <preference>
                <name>PREFdynamicSkinEnabled</name>
                <value></value>
            </preference>
            <!-- If PREFdynamic=true, the Dynamic Skin Portlet will process {PREFdynamicSkinName}.less to create
                 a custom skin CSS file to link into the page.  If dynamic=false, the Dynamic Skin Portlet
                 will link in {PREFdynamicSkinName}.css. -->
            <preference>
                <name>PREFdynamicSkinName</name>
                <value>defaultSkin</value>
            </preference>

            <!-- All PREF* preference values (without the PREF prefix) are dynamically injected as LESS variables
                 into the LESS compilation processing, but preferences below this comment are name/value pairs
                 that are intended to be used within the LESS files. -->

            <preference>
                <name>PREFcolor1</name>
                <value>#4065B0</value>
            </preference>
            <preference>
                <name>PREFcolor2</name>
                <value>#F68923</value>
            </preference>
            <preference>
                <name>PREFcolor3</name>
                <value>#428BCA</value>
            </preference>
            <preference>
                <name>PREFcolor4</name>
                <value>#ADADAD</value>
            </preference>
            <preference>
                <name>PREFcolor5</name>
                <value>#333333</value>
            </preference>
            <preference>
                <name>PREFcolor6</name>
                <value>#E5E5E5</value>
            </preference>
        </portlet-preferences>

