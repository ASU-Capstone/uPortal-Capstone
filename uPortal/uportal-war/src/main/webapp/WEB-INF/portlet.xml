        <display-name xml:lang="en">Session Timeout</display-name>
        <portlet-class>org.springframework.web.portlet.HeaderHandlingDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>classpath:/properties/contexts/portlet/SessionTimeout-portlet.xml</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
