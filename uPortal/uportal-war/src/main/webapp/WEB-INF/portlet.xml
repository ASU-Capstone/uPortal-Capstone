    <!-- 
      Simple, Spring Portlet MVC portlet implementation that renders a JSP at a 
      location specified as a portlet preference.  Very similar to the 
      SimpleJspPortlet in the jasig-widget-portlets project.
    -->
    <portlet>
        <portlet-name>JspInvoker</portlet-name>
        <display-name xml:lang="en">JspInvoker</display-name>
        <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>classpath:/properties/contexts/portlet/JspInvoker-portlet.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>JspInvoker</title>
        </portlet-info>
        <portlet-preferences>
            <!-- Location of the JSP file to use -->
            <preference>
                <name>JspInvokerPortletController.viewLocation</name>
            </preference>
            <!-- Names of String beans, if any, that should be made available to the JSP on the model -->
            <preference>
                <name>JspInvokerPortletController.beans</name>
            </preference>
            <!--
             | Security roles to test the user for as a convenience to allow the JSP to do basic security conditions
             | such as <c:if test=${isFaculty}></c:if> for role-sensitive menu operations or other needs.  Each role
             | listed here is checked and an "is"+roleName is added to the model sent to the JSP.  Each role
             | specified here must ALSO be specified in a <security-role-ref> element for this portlet.
             | Specify the roles to check in your portlet publication record.
             +-->
            <preference>
                <name>JspInvokerPortletController.securityRolesToTest</name>
                <!--<value>PortalSystem</value>-->
                <!--<value>FragmentOwners</value>-->
                <!--<value>TenantAdministrators</value>-->
                <!--<value>PortalAdministrators</value>-->
            </preference>
        </portlet-preferences>

        <!-- The role-name (the internal name used by the portlet) should not have spaces in it because
             the role-name is used to add isRoleName attributes to the model for use by the JSP -->

        <security-role-ref>
            <role-name>PortalSystem</role-name>
            <role-link>Portal System</role-link>
        </security-role-ref>

        <security-role-ref>
            <role-name>FragmentOwners</role-name>
            <role-link>Fragment Owners</role-link>
        </security-role-ref>

        <security-role-ref>
            <role-name>TenantAdministrators</role-name>
            <role-link>Tenant Administrators</role-link>
        </security-role-ref>

        <security-role-ref>
            <role-name>PortalAdministrators</role-name>
            <role-link>Portal Administrators</role-link>
        </security-role-ref>

    </portlet>

